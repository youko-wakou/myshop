<div class="col-md-12 col-xs-12"style="margin:15px 0;">
  <div class="col-md-6 col-xs-10 col-md-offset-3 col-xs-offset-1">
    <%= form_for(@support) do |f| %>
      <div class="form-group">
        <%= f.label :title %>
        <%= f.text_field :title,:class => 'form-control' %>
      </div>
      <div class="form-group">
        <%= f.label :content %>
        <%= f.text_area :content,:class => 'form-control'  %>
      </div>
      <%= f.submit 'Send',:class => 'button-change btn-lg btn-block',:style =>"border:none;" %>
    <% end %>
  </div>
</div>